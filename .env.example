# Backend & Database
POSTGRES_DB=entropyos
POSTGRES_USER=user
POSTGRES_PASSWORD=password
DB_PORT=5432
BACKEND_PORT=8000
DATABASE_URL=postgresql+psycopg2://user:password@db:5432/entropyos

# Frontend
FRONTEND_PORT=3000
# VITE_API_URL will be set inside the container based on BACKEND_PORT

# LLM Proxy
LLM_PROXY_PORT=8080
GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE # Replace with your actual key

# Hatchet
HATCHET_CLIENT_TENANT_ID=YOUR_HATCHET_TENANT_ID_HERE # Replace with your tenant ID
# Add any other Hatchet-specific env vars if needed (e.g., API endpoint if not default)

# Add other backend/frontend specific environment variables below as needed
# e.g., SECRET_KEY=your_secret_key 